<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@color/white"
    android:layout_height="match_parent"
    tools:context=".Lauth">
    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Welcome back"
        android:textColor="#000000"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/emailL"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.645" />

    <androidx.appcompat.widget.AppCompatEditText
        android:id="@+id/passL"
        android:layout_width="319dp"
        android:layout_height="63dp"
        android:background="@drawable/edit_text_back"
        android:ems="10"
        android:hint=" Password"
        android:inputType="text"
        android:textColor="#000000"
        android:textColorHint="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.401" />

    <androidx.appcompat.widget.AppCompatEditText
        android:id="@+id/passRL"
        android:layout_width="319dp"
        android:layout_height="63dp"
        android:background="@drawable/edit_text_back"
        android:ems="10"
        android:hint=" Password(Retype)"
        android:inputType="textPassword"
        android:textColor="#000000"
        android:textColorHint="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.534" />

    <androidx.appcompat.widget.AppCompatEditText
        android:id="@+id/emailL"

        android:layout_width="319dp"
        android:layout_height="63dp"
        android:background="@drawable/edit_text_back"
        android:ems="10"
        android:hint=" Email"
        android:inputType="textPersonName"
        android:textColor="#000000"
        android:textColorHint="#000000"
        app:circularflow_radiusInDP="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.276" />

    <ImageView
        android:id="@+id/passShowbtnL"
        android:layout_width="34dp"
        android:layout_height="32dp"
        app:layout_constraintBottom_toBottomOf="@+id/passRL"
        app:layout_constraintEnd_toEndOf="@+id/passRL"
        app:layout_constraintHorizontal_bias="0.929"
        app:layout_constraintStart_toStartOf="@+id/passRL"
        app:layout_constraintTop_toBottomOf="@+id/passL"
        app:layout_constraintVertical_bias="0.629"
        app:srcCompat="@drawable/view" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/cl"
        android:layout_width="222dp"
        android:layout_height="73dp"
        android:background="@drawable/btnbak"
        android:text="Login"
        android:textAllCaps="false"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/passRL" />

</androidx.constraintlayout.widget.ConstraintLayout>